<h1>Login</h1>

<%= form_for @conn, session_path(@conn, :create), [as: :session], fn f -> %>
  <div class="">
    <%= email_input f, :email, placeholder: "Email", class: "" %>
  </div>

  <div class="">
    <%= password_input f, :password, placeholder: "Password", class: ""  %>
  </div>

  <%= submit "Log in", class: "" %>
<% end %>